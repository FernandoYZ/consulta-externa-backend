{
  "name": "backend",
  "version": "1.0.50",
  "scripts": {
    "start": "bun src/server.ts",
    "dev": "bun --watch src/server.ts",
    "prod": "NODE_ENV=production bun src/server.ts",
    "logs:errors": "cat logs/errors.log 2>/dev/null || echo 'No hay errores registrados'",
    "logs:warnings": "cat logs/warnings.log 2>/dev/null || echo 'No hay warnings registrados'",
    "logs:clear": "rm -rf logs/*.log && echo 'Logs eliminados'"
  },
  "dependencies": {
    "@types/mssql": "^9.1.8",
    "elysia": "latest",
    "eta": "^4.0.1",
    "mssql": "^12.0.0"
  },
  "devDependencies": {
    "@types/bun": "^1.3.1",
    "bun-types": "latest"
  },
  "module": "src/index.js"
}
